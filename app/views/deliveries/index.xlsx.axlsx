wb = xlsx_package.workbook

wb.add_worksheet(name: "عمليات التسليم") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row ["comodity_type","amount","delegate","delivery time"]
  
  # each user is a row on your spreadsheet
  @deliveries.each do |delivery|
    sheet.add_row [ delivery.commodity_type, delivery.commodity_amount , delivery.delegate.name , delivery.delivery_time.to_date]
  end
end